package carte;

import javax.swing.JButton;

import utils.Constantes.Type;

public class Case extends JButton {
	private Type type; 
	private int x;
	private int y;
	
	public Case (Type type, int x, int y, Sprites sprite) {
		this.type = type;
		this.x = x;
		this.y = y;
		this.setIcon(sprite.getSpriteCaseChemin());
	}

	public Type getType() {
		return this.type;
	}

	public int getX() {
		return this.x;
	}

	public int getY() {
		return this.y;
	}
	
	
}
